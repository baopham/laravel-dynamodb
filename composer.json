{
    "name": "baopham/dynamodb",
    "description": "DynamoDb wrapper for your Laravel model and helpers",
    "keywords": ["laravel", "dynamodb", "aws"],
    "require": {
        "aws/aws-sdk-php": "^3.0.0",
        "illuminate/support": "~5.1",
        "illuminate/database": "~5.1",
        "paragonie/random_compat": "^2.0.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bao Pham",
            "email": "gbaopham@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BaoPham\\DynamoDb\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^5.0@dev"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/ModelTest.php",
            "tests/DynamoDbModelTest.php"
        ]
    }
}
